{
   "data": {
      "aws_iam_policy_document": {
         "codebuild_assume_role": {
            "statement": {
               "actions": [
                  "sts:AssumeRole"
               ],
               "effect": "Allow",
               "principals": [
                  {
                     "identifiers": [
                        "codebuild.amazonaws.com"
                     ],
                     "type": "Service"
                  }
               ],
               "sid": "1"
            }
         },
         "codebuild_policy": {
            "statement": [
               {
                  "actions": [
                     "logs:CreateLogGroup",
                     "logs:CreateLogStream",
                     "logs:PutLogEvents"
                  ],
                  "effect": "Allow",
                  "resources": [
                     "${aws_cloudwatch_log_group.hello_codebuild.arn}",
                     "${aws_cloudwatch_log_stream.codebuild.arn}"
                  ]
               },
               {
                  "actions": [
                     "s3:PutObject",
                     "s3:GetObject",
                     "s3:GetObjectVersion"
                  ],
                  "effect": "Allow",
                  "resources": [
                     "${aws_s3_bucket.pipeline.arn}",
                     "${aws_s3_bucket.pipeline.arn}/*"
                  ]
               }
            ]
         },
         "codepipeline_assume_role": {
            "statement": {
               "actions": [
                  "sts:AssumeRole"
               ],
               "effect": "Allow",
               "principals": [
                  {
                     "identifiers": [
                        "codepipeline.amazonaws.com"
                     ],
                     "type": "Service"
                  }
               ],
               "sid": "1"
            }
         },
         "codepipeline_policy": {
            "statement": [
               {
                  "actions": [
                     "s3:PutBucketPolicy",
                     "s3:PutObject"
                  ],
                  "effect": "Allow",
                  "resources": [
                     "${aws_cloudwatch_log_group.hello_codebuild.arn}",
                     "${aws_cloudwatch_log_group.hello_codebuild.arn}/*"
                  ]
               },
               {
                  "actions": [
                     "codecommit:GetBranch",
                     "codecommit:GetCommit",
                     "codecommit:UploadArchive",
                     "codecommit:GetUploadArchiveStatus",
                     "codecommit:CancelUploadArchive"
                  ],
                  "effect": "Allow",
                  "resources": [
                     "${aws_codecommit_repository.main.arn}"
                  ]
               },
               {
                  "actions": [
                     "codebuild:StartBuild",
                     "codebuild:BatchGetBuilds"
                  ],
                  "effect": "Allow",
                  "resources": [
                     "${aws_codebuild_project.test_go_code.arn}"
                  ]
               },
               {
                  "actions": [
                     "s3:PutObject",
                     "s3:GetObject",
                     "s3:GetObjectVersion"
                  ],
                  "effect": "Allow",
                  "resources": [
                     "${aws_s3_bucket.pipeline.arn}",
                     "${aws_s3_bucket.pipeline.arn}/*"
                  ]
               }
            ]
         }
      }
   },
   "provider": {
      "aws": {
         "region": "us-east-1"
      }
   },
   "resource": {
      "aws_cloudwatch_log_group": {
         "hello_codebuild": {
            "name": "akerr-lab-codebuild-hello-log-group"
         }
      },
      "aws_cloudwatch_log_stream": {
         "codebuild": {
            "log_group_name": "${aws_cloudwatch_log_group.hello_codebuild.name}",
            "name": "akerr-lab-codebuild-hello-log-stream-01"
         }
      },
      "aws_codebuild_project": {
         "test_go_code": {
            "artifacts": {
               "type": "CODEPIPELINE"
            },
            "build_timeout": "5",
            "cache": {
               "location": "${aws_s3_bucket.build_test_cache.bucket}",
               "type": "S3"
            },
            "description": "Run go unit tests.",
            "environment": {
               "compute_type": "BUILD_GENERAL1_SMALL",
               "image": "aws/codebuild/amazonlinux2-x86_64-standard:2.0",
               "image_pull_credentials_type": "CODEBUILD",
               "type": "LINUX_CONTAINER"
            },
            "logs_config": {
               "cloudwatch_logs": {
                  "group_name": "${aws_cloudwatch_log_group.hello_codebuild.name}",
                  "status": "ENABLED",
                  "stream_name": "${aws_cloudwatch_log_stream.codebuild.name}"
               }
            },
            "name": "akerr-lab-hello-test-gocode",
            "service_role": "${aws_iam_role.codebuild_role.arn}",
            "source": {
               "buildspec": "\"phases\":\n  \"install\":\n    \"runtime-versions\":\n      \"golang\": \"1.13\"\n  \"pre_build\":\n    \"commands\":\n    - \"cd $${CODEBUILD_SRC_DIR}\"\n    - \"ls\"\n    - \"make test\"\n\"version\": \"0.2\"",
               "type": "CODEPIPELINE"
            }
         }
      },
      "aws_codecommit_repository": {
         "main": {
            "description": "sample lambda hello app.",
            "repository_name": "hello"
         }
      },
      "aws_codepipeline": {
         "dev": {
            "artifact_store": {
               "location": "${aws_s3_bucket.pipeline.bucket}",
               "type": "S3"
            },
            "name": "lab-hello-dev-pipeline",
            "role_arn": "${aws_iam_role.codepipeline_role.arn}",
            "stage": [
               {
                  "action": [
                     {
                        "category": "Source",
                        "configuration": {
                           "BranchName": "dev",
                           "RepositoryName": "hello"
                        },
                        "name": "source-code",
                        "output_artifacts": [
                           "source-output"
                        ],
                        "owner": "AWS",
                        "provider": "CodeCommit",
                        "run_order": 1,
                        "version": "1"
                     }
                  ],
                  "name": "Source"
               },
               {
                  "action": [
                     {
                        "category": "Test",
                        "configuration": {
                           "ProjectName": "akerr-lab-hello-test-gocode"
                        },
                        "input_artifacts": [
                           "source-output"
                        ],
                        "name": "app-test-gocode",
                        "owner": "AWS",
                        "provider": "CodeBuild",
                        "run_order": 2,
                        "version": "1"
                     }
                  ],
                  "name": "Test"
               }
            ]
         }
      },
      "aws_iam_policy": {
         "codebuild": {
            "name": "codebuild-hello",
            "policy": "${data.aws_iam_policy_document.codebuild_policy.json}"
         },
         "codepipeline": {
            "name": "codepipeline-hello",
            "policy": "${data.aws_iam_policy_document.codepipeline_policy.json}"
         }
      },
      "aws_iam_role": {
         "codebuild_role": {
            "assume_role_policy": "${data.aws_iam_policy_document.codebuild_assume_role.json}",
            "name": "akerr-lab-codebuild-hello"
         },
         "codepipeline_role": {
            "assume_role_policy": "${data.aws_iam_policy_document.codepipeline_assume_role.json}",
            "name": "akerr-lab-codepipeline-hello"
         }
      },
      "aws_iam_role_policy_attachment": {
         "codebuild": {
            "policy_arn": "${aws_iam_policy.codebuild.arn}",
            "role": "${aws_iam_role.codebuild_role.name}"
         },
         "codepipeline": {
            "policy_arn": "${aws_iam_policy.codepipeline.arn}",
            "role": "${aws_iam_role.codepipeline_role.name}"
         }
      },
      "aws_s3_bucket": {
         "build_test_cache": {
            "acl": "private",
            "bucket": "akerr-lab-hello-build-test-cache"
         },
         "pipeline": {
            "acl": "private",
            "bucket": "akerr-lab-hello-pipeline"
         }
      }
   },
   "terraform": {
      "backend": {
         "s3": {
            "bucket": "akerr-lab-tfstate",
            "key": "repos/hello/terraform.tfstate",
            "region": "us-east-1"
         }
      }
   }
}
